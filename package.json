{
  "name": "@hitsokeyco/ozenpick",
  "version": "0.1.11",
  "private": false,
  "type": "module",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "description": "Una librer√≠a de componentes React moderna y personalizable, construida con TypeScript y CSS puro",
  "author": "Sergio <sergio@ozenpick.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hitsokeyco/ozenpick.git"
  },
  "bugs": {
    "url": "https://github.com/hitsokeyco/ozenpick/issues"
  },
  "homepage": "https://github.com/hitsokeyco/ozenpick#readme",
  "keywords": [
    "react",
    "component-library",
    "ui",
    "vite",
    "typescript",
    "styled",
    "components",
    "react-components",
    "ui-library",
    "design-system",
    "ozenpick"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./dist/ozenpick.css": {
      "import": "./dist/ozenpick.css",
      "require": "./dist/ozenpick.css"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "tsc -p tsconfig.dev.json --watch & vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "demo": "npm run build && vite --config vite.demo.config.ts"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/builder-vite": "^9.1.10",
    "@storybook/react": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-storybook": "^9.1.10",
    "storybook": "^9.1.10",
    "terser": "^5.44.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.45.0",
    "vite": "^6.3.1",
    "vite-plugin-dts": "^3.7.3"
  }
}
